<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <!-- Start fake_bbox_angles node -->
  <node name="fake_bbox_angles" pkg="navigation_pkg" type="fake_bbox_angles_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>

  <!-- Start fake_robot_coords node -->
  <node name="fake_robot_coords" pkg="navigation_pkg" type="fake_robot_coords_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>

  <!-- Start fake_yolo_boxes node -->
  <node name="fake_yolo_boxes" pkg="navigation_pkg" type="fake_yolo_boxes_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>

  <!-- Start fake_compass_headings node -->
  <node name="fake_compass_headings" pkg="navigation_pkg" type="fake_compass_headings_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>

  <node pkg="urg_node" type="urg_node" name="urg_node" output="screen">
        <param name="ip_address" value="192.168.0.10" />
  </node>

  <!-- Start angle_finder node -->
  <node name="angle_finder" pkg="navigation_pkg" type="angle_finder_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>

  <!-- Start distance_finder node -->
  <node name="distance_finder" pkg="navigation_pkg" type="distance_finder_node" output="screen">
    <!-- Add any necessary arguments here -->
  </node>


  <!-- Start coord_finder node -->
  <node name="coord_finder" pkg="navigation_pkg" type="coord_finder_node" output="log">
    <!--param name = "degrees_lat_per_meter" value="6.2"/>
    <param name = "degrees_lon_per_meter" value="5.2"/>
    <param name = "safety_range_meters" value="0.1"/-->

  </node>

   <!-- Start prop_mapping node -->
  <node name="prop_mapping" pkg="navigation_pkg" type="prop_mapping_node" output="log">
    <!-- Add any necessary arguments here -->
  </node>
</launch>
